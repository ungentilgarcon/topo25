{
  "name": "topogram-client",
  "version": "0.9.0",
  "description": "Real-time collaborative social network analysis toolkit",
  "main": "topogram.js",
  "dependencies": {
    "cytoscape": "2.7.2",
    "cytoscape-cola": "^1.4.1",
    "cytoscape-cxtmenu": "^2.7.1",
    "cytoscape-edgehandles": "^2.4.2",
    "cytoscape-spread": "^1.2.0",
    "cytoscape-springy": "^1.2.1",
    "d3": "^3.5.17",
    "geojson-tools": "^0.2.2",
    "jquery": "^2.2.4",
    "leaflet": "^0.7.7",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.1",
    "meteor-node-stubs": "^0.2.3",
    "moment": "^2.13.0",
    "nouislider": "^8.5.1",
    "papaparse": "^4.1.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-mounter": "^1.2.0",
    "react-tap-event-plugin": "^1.0.0",
    "webcola": "^3.1.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "daemon": "^1.1.0",
    "eslint": "^2.11.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-meteor": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-jshint": "^1.8.5",
    "gulp-mocha": "^2.2.0",
    "gulp-prompt": "^0.1.1",
    "gulp-replace": "^0.4.0",
    "gulp-shell": "^0.2.9",
    "jshint-stylish": "^1.0.0",
    "run-sequence": "^1.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "meteor",
      "react"
    ],
    "globals": {
      "React": true
    },
    "ecmaFeatures": {
      "jsx": true
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "no-console": 0,
      "meteor/eventmap-params": [
        2,
        {
          "eventParamName": "event",
          "templateInstanceParamName": "instance"
        }
      ],
      "meteor/template-names": [
        0
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ]
    }
  },
  "scripts": {
    "test": "meteor test --driver-package practicalmeteor:mocha",
    "lint": "eslint .",
    "start": "meteor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topogram/topogram-client.git"
  },
  "author": "Cl√©ment Renaud <hi@clementrenaud.com> (http://clementrenaud.com/)",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/topogram/topogram-client/issues"
  },
  "homepage": "https://github.com/topogram/topogram-client"
}
