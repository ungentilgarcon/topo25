{
  "name": "topogram-client",
  "version": "1.0.0",
  "description": "Real-time collaborative social network analysis toolkit",
  "main": "topogram.js",
  "dependencies": {
    "cytoscape": "2.7.2",
    "cytoscape-arbor": "^1.2.1",
    "cytoscape-cose-bilkent": "^1.3.3",
    "cytoscape-cxtmenu": "^2.7.1",
    "cytoscape-edgehandles": "2.4.1",
    "cytoscape-spread": "^1.2.0",
    "cytoscape-springy": "^1.2.1",
    "d3": "^3.5.17",
    "geojson-tools": "^0.2.2",
    "jquery": "^2.2.4",
    "lodash": "^4.13.1",
    "meteor-node-stubs": "^0.2.3",
    "moment": "^2.13.0",
    "nouislider": "^8.5.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-meteor": "^3.0.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "no-console": 0,
      "meteor/eventmap-params": [
        2,
        {
          "eventParamName": "event",
          "templateInstanceParamName": "instance"
        }
      ],
      "meteor/template-names": [
        0
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ]
    }
  },
  "scripts": {
    "test": "meteor test --driver-package practicalmeteor:mocha",
    "lint": "eslint ./imports",
    "start": "meteor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topogram/topogram-client.git"
  },
  "author": "Cl√©ment Renaud <hi@clementrenaud.com> (http://clementrenaud.com/)",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/topogram/topogram-client/issues"
  },
  "homepage": "https://github.com/topogram/topogram-client"
}
